###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#PartitaAmichevoleLibera
scacchi2023:PartitaAmichevoleLibera rdf:type owl:Class ;
                                    rdfs:subClassOf scacchi2023:PartitaAmichevole .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#PartitaUfficiale
scacchi2023:PartitaUfficiale rdf:type owl:Class ;
                             rdfs:subClassOf scacchi2023:Partita .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#PartitaUfficialeBlitz
scacchi2023:PartitaUfficialeBlitz rdf:type owl:Class ;
                                  owl:equivalentClass [ owl:intersectionOf ( scacchi2023:PartitaUfficiale
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty scacchi2023:tempoPerGiocatore ;
                                                                               owl:hasValue 1
                                                                             ]
                                                                           ) ;
                                                        rdf:type owl:Class
                                                      ] ;
                                  rdfs:subClassOf scacchi2023:PartitaUfficiale .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#PartitaUfficialeGiocoRapido
scacchi2023:PartitaUfficialeGiocoRapido rdf:type owl:Class ;
                                        owl:equivalentClass [ owl:intersectionOf ( scacchi2023:PartitaUfficiale
                                                                                   [ rdf:type owl:Restriction ;
                                                                                     owl:onProperty scacchi2023:tempoPerGiocatore ;
                                                                                     owl:hasValue 10
                                                                                   ]
                                                                                 ) ;
                                                              rdf:type owl:Class
                                                            ] ;
                                        rdfs:subClassOf scacchi2023:PartitaUfficiale .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#PartitaUfficialeStandard
scacchi2023:PartitaUfficialeStandard rdf:type owl:Class ;
                                     owl:equivalentClass [ owl:intersectionOf ( scacchi2023:PartitaUfficiale
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty scacchi2023:tempoPerGiocatore ;
                                                                                  owl:hasValue 60
                                                                                ]
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ] ;
                                     rdfs:subClassOf scacchi2023:PartitaUfficiale .


