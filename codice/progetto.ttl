###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#Partita
scacchi2023:Partita rdf:type owl:Class ;
                    owl:equivalentClass <https://www.bbc.co.uk/ontologies/sport/Match> ;
                    rdfs:label "Partita" .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#PartitaAmichevole
scacchi2023:PartitaAmichevole rdf:type owl:Class ;
                              rdfs:subClassOf scacchi2023:Partita ;
