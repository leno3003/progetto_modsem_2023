#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf
<http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf> rdf:type owl:ObjectProperty ,
                                                                            owl:TransitiveProperty ;
                                                                   rdfs:domain owl:Thing ;
                                                                   rdfs:range owl:Thing .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#elementoDi
scacchi2023:elementoDi rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf> ;
                       rdfs:domain scacchi2023:Strategia ;
                       rdfs:range scacchi2023:Partita .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#giocatoreGiocaPartita
scacchi2023:giocatoreGiocaPartita rdf:type owl:ObjectProperty ;
                                  owl:inverseOf scacchi2023:partitaGiocataDaGiocatore ;
                                  rdfs:domain scacchi2023:Scacchista ;
                                  rdfs:range scacchi2023:Partita .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#giocatorePartecipaTorneo
scacchi2023:giocatorePartecipaTorneo rdf:type owl:ObjectProperty ;
                                     rdfs:domain scacchi2023:Scacchista ;
                                     rdfs:range scacchi2023:Torneo ;
                                     owl:propertyChainAxiom ( scacchi2023:giocatoreGiocaPartita
                                                              scacchi2023:partitaDiTorneo
                                                            ) .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#paeseOspitaTorneo
scacchi2023:paeseOspitaTorneo rdf:type owl:ObjectProperty ;
                              rdfs:domain scacchi2023:PaesiOspitanti ;
                              rdfs:range scacchi2023:Torneo .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#partitaDiTorneo
scacchi2023:partitaDiTorneo rdf:type owl:ObjectProperty ;
                            owl:inverseOf scacchi2023:torneoContienePartita ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:domain scacchi2023:Partita ;
                            rdfs:range scacchi2023:Torneo .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#partitaGiocataDaGiocatore
scacchi2023:partitaGiocataDaGiocatore rdf:type owl:ObjectProperty ;
                                      rdfs:domain scacchi2023:Partita ;
                                      rdfs:range scacchi2023:Scacchista .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#strategiaPartitaBianco
scacchi2023:strategiaPartitaBianco rdf:type owl:ObjectProperty ;
                                   rdfs:domain scacchi2023:Strategia ;
                                   rdfs:range scacchi2023:Partita .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#strategiaPartitaNero
scacchi2023:strategiaPartitaNero rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf owl:topObjectProperty ;
                                 rdfs:domain scacchi2023:Strategia ;
                                 rdfs:range scacchi2023:Partita .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#torneoContienePartita
scacchi2023:torneoContienePartita rdf:type owl:ObjectProperty ,
                                           owl:InverseFunctionalProperty ;
                                  rdfs:domain scacchi2023:Torneo ;
                                  rdfs:range scacchi2023:Partita .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#vincitorePartita
scacchi2023:vincitorePartita rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf owl:topObjectProperty ;
                             rdfs:domain scacchi2023:Scacchista ;
                             rdfs:range scacchi2023:Partita .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#vincitoreTorneo
scacchi2023:vincitoreTorneo rdf:type owl:ObjectProperty ;
                            rdfs:domain scacchi2023:Scacchista ;
                            rdfs:range scacchi2023:Torneo .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#annoTorneo
scacchi2023:annoTorneo rdf:type owl:DatatypeProperty ;
                       rdfs:domain scacchi2023:Torneo ;
                       rdfs:range xsd:int .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#nomeCompleto
scacchi2023:nomeCompleto rdf:type owl:DatatypeProperty ;
                         rdfs:domain scacchi2023:Scacchista ;
                         rdfs:range xsd:string .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#numeroPartecipanti
scacchi2023:numeroPartecipanti rdf:type owl:DatatypeProperty ;
                               rdfs:domain scacchi2023:Torneo ;
                               rdfs:range xsd:nonNegativeInteger .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#punteggioElo
scacchi2023:punteggioElo rdf:type owl:DatatypeProperty ;
                         rdfs:domain scacchi2023:ScacchistaProfessionista ;
                         rdfs:range [ rdf:type rdfs:Datatype ;
                                      owl:onDatatype xsd:nonNegativeInteger ;
                                      owl:withRestrictions ( [ xsd:maxInclusive "4000"^^xsd:nonNegativeInteger
                                                             ]
                                                           )
                                    ] .


###  http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#tempoPerGiocatore
scacchi2023:tempoPerGiocatore rdf:type owl:DatatypeProperty ;
                              rdfs:domain scacchi2023:PartitaUfficiale ;
                              rdfs:range xsd:nonNegativeInteger ;
                              rdfs:comment "Tempo iniziale per ciascun giocatore durante la partita. L'unità di misura è minuti." .


