                                                                              rdfs:domain <http://www.semanticweb.org/leno/ontologies/2023/11/scacchi2023#ScacchistaProfessionista> ;
                                                                              rdfs:range [ rdf:type rdfs:Datatype ;
                                                                                           owl:onDatatype xsd:nonNegativeInteger ;
